{
  "name": "goalapi/opendata",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Murat Erkenov",
      "email": "murat@11bits.net"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/console": "^5.0",
    "symfony/framework-bundle": "^5.0",
    "symfony/monolog-bundle": "^3.5",
    "goalapi/symfony-sdk-bundle": "dev-master",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.6",
    "phpunit/phpunit": "^6.0",
    "symfony/debug": "^4.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:goalapi/symfony-sdk.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
